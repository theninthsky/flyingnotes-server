{
  "name": "flyingnotes-server",
  "version": "1.0.0",
  "author": "Almog Gabay",
  "license": "ISC",
  "engines": {
    "node": "15.8.0"
  },
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "npm-run-all --sequential build serve",
    "dev": "npm-run-all --parallel build:watch serve:watch",
    "build": "tsc",
    "build:watch": "tsc -w",
    "serve": "node -r dotenv/config dist/server",
    "serve:watch": "nodemon -r dotenv/config dist/server",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test mocha -r dotenv/config",
    "publish": "tsc && npm run test && git push"
  },
  "dependencies": {
    "ajv": "^7.1.0",
    "ajv-formats": "^1.5.1",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "fast-json-stable-stringify": "^2.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.4",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.28",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.3.0",
    "mocha": "^7.2.0",
    "mongodb-memory-server": "^6.9.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.5"
  }
}
